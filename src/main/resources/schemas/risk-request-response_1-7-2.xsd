    </xsd:complexType>
    <xsd:complexType name="current-address-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element ref="first-name" minOccurs="0"/>
            <xsd:element ref="middle-name" minOccurs="0"/>
            <xsd:element ref="last-name" minOccurs="0"/>
            <xsd:element ref="company-name" minOccurs="0"/>
            <xsd:element ref="street-name" minOccurs="0"/>
            <xsd:element ref="street-number" minOccurs="0"/>
            <xsd:element ref="street-number-ext" minOccurs="0"/>
            <xsd:element ref="apartment-number" minOccurs="0"/>
            <xsd:element ref="address-ext" minOccurs="0"/>
            <xsd:element ref="zip-code" minOccurs="0"/>
            <xsd:element ref="po-box" minOccurs="0"/>
            <xsd:element ref="city" minOccurs="0"/>
            <xsd:element ref="state" minOccurs="0"/>
            <xsd:element ref="country" minOccurs="0"/>
            <xsd:element ref="confidence-factor" minOccurs="0"/>
            <xsd:element ref="address-validity" minOccurs="0"/>
            <xsd:element ref="address-detail" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="duration" type="xsd:long"/>
    <xsd:complexType name="identity-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ip-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element ref="aol" minOccurs="0"/>
            <xsd:element ref="city" minOccurs="0"/>
            <xsd:element ref="country" minOccurs="0"/>
            <xsd:element ref="country-confidence-factor" minOccurs="0"/>
            <xsd:element ref="ip-address" minOccurs="0"/>
            <xsd:element ref="zip-code" minOccurs="0"/>
            <xsd:element ref="ip-routing-method" minOccurs="0"/>
            <xsd:element ref="anonymizer-status" minOccurs="0"/>
            <xsd:element name="carrier" type="xsd:string" minOccurs="0"/>
            <xsd:element name="asn" type="xsd:string" minOccurs="0"/>
            <xsd:element name="topLevelDomain" type="xsd:string" minOccurs="0"/>
            <xsd:element name="secondLevelDomain" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="response-id" type="xsd:string"/>
    <xsd:element name="return-code" type="xsd:int"/>
    <xsd:element name="decision" type="xsd:int"/>
    <xsd:element name="internal-error-code" type="xsd:string"/>
    <xsd:element name="internal-error-message" type="xsd:string"/>
    <xsd:element name="return-message" type="xsd:string"/>
    <xsd:element name="score" type="xsd:long"/>
    <xsd:element name="originalScore" type="xsd:long"/>
    <xsd:element name="score-class-code" type="xsd:int"/>
    <xsd:element name="score-range-code" type="xsd:string"/>
    <xsd:complexType name="selectType">
        <xsd:sequence>
            <xsd:element ref="choice" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="ship-to-address-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element ref="first-name" minOccurs="0"/>
            <xsd:element ref="middle-name" minOccurs="0"/>
            <xsd:element ref="last-name" minOccurs="0"/>
            <xsd:element ref="street-name" minOccurs="0"/>
            <xsd:element ref="street-number" minOccurs="0"/>
            <xsd:element ref="apartment-number" minOccurs="0"/>
            <xsd:element ref="zip-code" minOccurs="0"/>
            <xsd:element ref="po-box" minOccurs="0"/>
            <xsd:element ref="city" minOccurs="0"/>
            <xsd:element ref="state" minOccurs="0"/>
            <xsd:element ref="country" minOccurs="0"/>
            <xsd:element ref="confidence-factor" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="credit-limitType">
        <xsd:sequence>
            <xsd:element name="amount" type="amountType" minOccurs="0"/>
            <xsd:element name="outstanding-order-amount" type="amountType" minOccurs="0"/>
            <xsd:element name="credit-amount" type="amountType" minOccurs="0"/>
            <xsd:element name="current-credit-limit-amount" type="amountType" minOccurs="0"/>
            <xsd:element name="current-credit-limit-amount-aux" type="amountType" minOccurs="0"/>
            <xsd:element name="current-credit-limit-extendable" type="xsd:boolean" nillable="true" minOccurs="0"/>
            <xsd:element ref="dunning-level" minOccurs="0"/>
            <xsd:element name="goods" type="goodsType" minOccurs="0"/>
            <xsd:element ref="order-time" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="class-of-goods" type="xsd:string"/>
    <xsd:element name="dunning-level" type="xsd:string"/>
    <xsd:complexType name="goodsType">
        <xsd:sequence>
            <xsd:element ref="class-of-goods" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="order-time" type="wdDateTime"/>
    <xsd:element name="credit-limit-code" type="xsd:string"/>
    <xsd:complexType name="credit-limit-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element ref="credit-limit-code" minOccurs="0"/>
            <xsd:element name="credit-limit-amount" type="amountType" minOccurs="0"/>
            <xsd:element name="credit-limit-amount-aux" type="amountType" minOccurs="0"/>
            <xsd:element name="credit-limit-extendable" type="xsd:boolean" minOccurs="0"/>
            <xsd:element name="credit-limit-comment" type="xsd:string" minOccurs="0"/>
            <xsd:element ref="last-scoring-date" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="doublet-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element name="phone-doublet-count" type="xsd:integer" minOccurs="0"/>
            <xsd:element name="address-doublet-count" type="xsd:integer" minOccurs="0"/>
            <!-- <xsd:element name="customer-id-and-phone" type="xsd:integer" minOccurs="0"/> -->
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="hotlist-verificationType">
        <xsd:sequence>
            <xsd:element name="address-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="bank-account-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="sepa-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="creditcard-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="customer-id-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="email-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="ip-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="bin-country-code-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="ip-country-code-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="device-id-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="sanction-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="phone-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
            <xsd:element name="passenger-hotlist" type="hotlistResultType" maxOccurs="1" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="hotlistResultType">
        <xsd:sequence>
            <xsd:element name="blacklist-match" type="hotlist-match" minOccurs="0"/>
            <xsd:element name="whitelist-match" type="hotlist-match" minOccurs="0"/>
            <xsd:element name="greylist-match" type="hotlist-match" minOccurs="0"/>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="hotlist-match">
        <xsd:simpleContent>
            <xsd:extension base="xsd:boolean">
                <xsd:attribute name="label" use="optional" type="xsd:string"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="check-3ds-enrollment-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element name="transaction-status" type="enrollment-transaction-statusType" minOccurs="0"/>
            <xsd:element name="acs-url" type="xsd:string" minOccurs="0"/>
            <xsd:element name="xid" type="xsd:string" minOccurs="0"/>
            <xsd:element name="pareq" type="xsd:string" minOccurs="0"/>
            <xsd:element name="card-in-cache" type="xsd:boolean" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="enrollment-transaction-statusType">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
    <xsd:complexType name="check-3ds-authentication-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element name="transaction-status" type="authentication-transaction-statusType" minOccurs="0"/>
            <xsd:element name="eci" type="xsd:string" minOccurs="0"/>
            <xsd:element name="xid" type="xsd:string" minOccurs="0"/>
            <xsd:element name="acs-verification-id" type="xsd:string" minOccurs="0"/>
            <xsd:element name="purchase-amount" type="amountType" minOccurs="0"/>
            <xsd:element name="acs-verification-algorithm" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:simpleType name="authentication-transaction-statusType">
        <xsd:restriction base="xsd:string"/>
    </xsd:simpleType>
    <xsd:element name="mandate" type="mandateType"/>
    <xsd:complexType name="mandateType">
        <xsd:sequence>
            <xsd:element ref="parameter" minOccurs="0" maxOccurs="unbounded"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="parameter" type="parameterType"/>
    <xsd:complexType name="parameterType">
        <xsd:simpleContent>
            <xsd:extension base="xsd:string">
                <xsd:attribute name="name" type="xsd:string" use="required"/>
            </xsd:extension>
        </xsd:simpleContent>
    </xsd:complexType>
    <xsd:complexType name="fraud-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element ref="parameter" minOccurs="0" maxOccurs="unbounded"/>
            <!-- added for fps -->
            <xsd:element name="intercept" type="xsd:int" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="reason-code" type="xsd:int" minOccurs="0" maxOccurs="1"/>
            <xsd:element name="reason-message" type="xsd:string" minOccurs="0" maxOccurs="1"/>
            <!-- new ext for score + reason-code-list -->
            <xsd:element name="score" type="scoreType" minOccurs="0"/>
            <xsd:element name="reason-code-list" type="xsd:string" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="device-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element name="device-alias" type="xsd:string" minOccurs="0" maxOccurs="1"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="pnr-file-key" type="lengthRestrictedStringType256"/>
    <xsd:element name="agent-code" type="agent-codeType"/>
    <xsd:element name="passenger-name" type="lengthRestrictedStringType256"/>
    <xsd:element name="passenger-email" type="xsd:string"/>
    <xsd:element name="originating-airport" type="fixedLenghtStringType3"/>
    <xsd:simpleType name="lengthRestrictedStringType256">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="256"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="agent-codeType">
        <xsd:restriction base="xsd:string">
            <xsd:pattern value="[0-9]*"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="maxLenghtStringType3">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="3"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="maxLenghtStringType32">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="32"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="maxLenghtStringType1">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="1"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="fixedLenghtStringType3">
        <xsd:restriction base="xsd:string">
            <xsd:length value="3"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:complexType name="airline-itineraryType">
        <xsd:sequence>
            <xsd:element name="segment" type="airline-segmentType" maxOccurs="unbounded" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:complexType name="airline-segmentType">
        <xsd:sequence>
            <xsd:element ref="class-of-service" minOccurs="0"/>
            <xsd:element ref="leg-departure-date" minOccurs="0"/>
            <xsd:element ref="leg-departure-time" minOccurs="0"/>
            <xsd:element ref="destination" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>
    <xsd:element name="class-of-service" type="maxLenghtStringType3"/>
    <xsd:element name="leg-departure-date" type="wdDate"/>
    <xsd:element name="leg-departure-time" type="wdTime"/>
    <xsd:element name="destination" type="maxLenghtStringType3"/>

    <xsd:simpleType name="fixedLenghtStringType1">
        <xsd:restriction base="xsd:string">
            <xsd:length value="1"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="fixedLenghtStringType2">
        <xsd:restriction base="xsd:string">
            <xsd:length value="2"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:element name="frequent-flyer-id" type="maxLenghtStringType20"/>
    <xsd:element name="number-of-tickets" type="oneOrTwoDigitsType"/>

    <xsd:simpleType name="oneOrTwoDigitsType">
        <xsd:restriction base="xsd:int">
            <xsd:pattern value="[0-9][0-9]?"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:element name="one-way" type="xsd:boolean" nillable="true"/>

    <xsd:simpleType name="maxLenghtStringType20">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="20"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="paymentGroupIdType">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="36"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="debitReturn-reason-codeType">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="100" />
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="fraud-typeType">
        <xsd:restriction base="xsd:string">
            <xsd:maxLength value="256"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="chargeback-reason-codeType">
        <xsd:restriction base="xsd:integer">
            <xsd:maxInclusive value="9999"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:simpleType name="liability-shiftType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="N"/>
            <xsd:enumeration value="Y"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="recurring-typeType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="single"/>
            <xsd:enumeration value="initial"/>
            <xsd:enumeration value="repeated"/>
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="payment-typeType">
        <xsd:restriction base="xsd:string">
            <xsd:enumeration value="preauthorization"/>
            <xsd:enumeration value="capture-preauthorization"/>
            <xsd:enumeration value="preauthorization-supplement"/>
            <xsd:enumeration value="capture-preauthorization-supplement"/>
            <xsd:enumeration value="authorization"/>
            <xsd:enumeration value="authorization-check"/>
            <xsd:enumeration value="capture-authorization"/>
            <xsd:enumeration value="purchase"/>
            <xsd:enumeration value="notification"/>
            <xsd:enumeration value="bookback"/>
            <xsd:enumeration value="reversal"/>
            <xsd:enumeration value="oct"/>
            <xsd:enumeration value="query"/>
            <xsd:enumeration value="refund"/>
            <xsd:enumeration value="chargeback"/>
            <xsd:enumeration value="enrollment-check"/>
            <xsd:enumeration value="direct-debit"/>
            <xsd:enumeration value="debit-return"/>
            <xsd:enumeration value="fraud"/>
        </xsd:restriction>
    </xsd:simpleType>

    <xsd:complexType name="age-verificationType">
        <xsd:sequence>
            <xsd:element ref="return-code"/>
            <xsd:element ref="return-message"/>
            <xsd:element name="age" type="xsd:int" minOccurs="0"/>
            <xsd:element ref="processing-hint" minOccurs="0"/>
            <xsd:element ref="processing-text" minOccurs="0"/>
        </xsd:sequence>
    </xsd:complexType>

    <xsd:element name="processing-hint" type="xsd:string"/>
    <xsd:element name="processing-text" type="xsd:string"/>

</xsd:schema>
